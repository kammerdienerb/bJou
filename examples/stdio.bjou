# stdio.bjou

# must be defined for clang.bjou to compile
const libclang_path := "/usr/local/Cellar/llvm/7.0.1/lib/libclang.dylib"

import "c2bjou.bjou"
\run{ c2bjou::include("/usr/include/stdio.h") }

main()

proc main() {
    f := fopen("out.txt", "w")

    the_num := 0
    printf("enter a number: ")
    scanf("%d", &the_num)
    f.fprintf("%d", the_num)

    f.fclose()
}
