# u642b32
# u64 to base 32 string

module u642b32

proc u642b32(u : u64) : string {
    _0 : u64 = '0'
    _a : u64 = 'a'
    s  := str()

    do {
        d := u % 32

        s = str(<< if d < 10 <-(_0 + d) else <-(_a + d - 10) >> as char)
            .concat(s)

        u /= 32
    } while u > 0

    return s
}
