# inheritinterface.bjou

interface I {
    proc i(this)
}

abstract type A {
    implements I {
        proc i(this)
            print "default"
    }
}

type T extends A {  }

type U extends A {
    implements I {
        proc i(this)
            print "override"
    }
}

proc main() {
    t := new T
    u := new U

    t.i()
    u.i()

    (t as A*).i()
    (u as A*).i()
}

main()