# inheritinterface.bjou

interface I {
    proc i(this)
}

abstract type A {
    implements I {
        proc i(this)
            print "default"
    }
}

type T extends A {  }

type U extends A {
    implements I {
        proc i(this)
            print "override"
    }
}

proc main() {
    t := getref(@ new T)
    u := getref(@ new U)

    t.i()
    u.i()

    refcast$A(t).i()
    refcast$A(u).i()
}

main()
