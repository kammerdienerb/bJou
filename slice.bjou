# slice.bjou

\run{ main() }

(proc main() {
    array := { 0, 1, 2, 3, 4, 5, 6, 7 }
    print "|array| = %", |array|
    slice := [ array, 2:3 ]
    print "|slice| = %", |slice|
    
    printf("slice view: [ ")
    comma := ", "
    for i := 0; i < |slice|; i += 1 {
        if i == |slice| - 1
            comma = " ]"
        printf("%d%s", slice[i], comma)
    }
    print ""
})()
