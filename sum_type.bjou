# sum_type.bjou

type A {}

proc p(a : A) : int {
    print "A"
    return 0
}

type B {}

proc p(a : B) {
    print "B"
    return 1
}

type C {}

proc p(a : C) {
    print "C"
    return 2
}

type T = ( A | B | C )

proc p(t : T) : int {
    return << match t {
        with a : A    <-a.p()
        with b : B    <-b.p()
        with c : C    <-c.p()
    } >>
}
